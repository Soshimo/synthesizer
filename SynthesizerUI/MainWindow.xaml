<mui:ModernWindow x:Class="SynthesizerUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SynthesizerUI" xmlns:viewmodel="clr-namespace:SynthesizerUI.ViewModel"
        xmlns:converters="clr-namespace:SynthesizerUI.Converters"
        xmlns:dependencyObjects="clr-namespace:SynthesizerUI.DependencyObjects"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        xmlns:controls="clr-namespace:SynthesizerUI.Controls"
        xmlns:windows="clr-namespace:SynthesizerUI.Windows"
        xmlns:mui="http://firstfloorsoftware.com/ModernUI"
        DataContext="{Binding Path=MainWindowViewModel, Source={StaticResource ViewModelLocator}}"
        mc:Ignorable="d"
        IsTitleVisible="True"
        ContentSource="Controls/SynthesizerPage.xaml"
        Style="{StaticResource SynthesizerModernWindow}"
        WindowStartupLocation="CenterScreen"
        Title="MainWindow" Height="980" Width="1080">



</mui:ModernWindow>